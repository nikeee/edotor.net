  "license": "MIT",
  "version": "1.7.1",
  "description": "Online Graphviz fiddle with language support",
  "private": true,
  "scripts": {
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider webpack -p",
    "build:dev": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode development",
    "watch": "NODE_OPTIONS=--openssl-legacy-provider webpack --watch --mode development",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "dot-language-support": "^1.5.2",
    "jquery": "^3.6.0",
    "monaco-editor": "~0.18.1",
    "monaco-languageclient": "~0.10.2",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-monaco-editor": "^0.34.0",
    "react-spinners": "^0.10.6",
    "react-split-pane": "^0.1.92",
    "svg-pan-zoom": "^3.6.1",
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "@types/inline-style-prefixer": "^5.0.0",
    "@types/jquery": "^3.5.5",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.1.0",
