    "style-loader": "^1.3.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  }
}
